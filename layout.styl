@import 'alpmixins'
@import 'jeet'
.main_path
  margin 0
  padding 0
  height 100%



.main_nav.mara
  display: inline-block
  width 100%
  position relative
  margin-bottom 35px
  padding 35px 0 0
  text-align center
  background-image url('http://localhost:8080/static/bg/bg-header.png')
  z-index 999

.main_nav.mara:after

  position: absolute
  left: 0
  bottom: -9px
  width: 100%
  height: 9px
  background-image: url('http://localhost:8080/static/bg/header-bottom-arrow.png');
  background-position: 0 0
  background-repeat: repeat-x
  content: ""
  z-index -1

.main_nav.mara .container
  width 960px
  margin 0 auto
  padding 0
  position relative

.nav_title.mara
  margin-left 35px
  &:hover
    cursor pointer
    cursor hand

.nav_title.mara .title_h1
  display: block
  float left
  margin: 0
  color: #e5e1d8
  font-size: 54px
  font-family: 'Over the Rainbow', cursive
  text-shadow: -1px -1px 0 #1d1d1d

.nav_btn_list.mara
  position: relative
  clear both
  top 30px
  width 500px
  margin: 0 -10px 0
  padding: 0 10px
  border-width: 1px
  border-style: solid
  border-color: #343131
  background: #f7f5ed
  background: -moz-linear-gradient(top,  #f7f5ed 0%, #ebe9e2 100%)
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7f5ed), color-stop(100%,#ebe9e2))
  background: -webkit-linear-gradient(top,  #f7f5ed 0%,#ebe9e2 100%)
  background: -o-linear-gradient(top,  #f7f5ed 0%,#ebe9e2 100%)
  background: -ms-linear-gradient(top,  #f7f5ed 0%,#ebe9e2 100%)
  border-radius(5px)
  box-shadow(0 0 3px rgba(0,0,0,.05))
  zoom: 1

.nav_btn_list.mara ._btn
  position: relative
  display: inline-block
  padding: 18px 22px
  text-align: center
  color: #36332e
  font-weight: 600
  font-size: 14px
  font-family: 'Open Sans', Arial
  text-transform: uppercase
  &:hover
    color blue
._btn_gr.mara
  text-align center
  margin 15px
._btn_gr.mara .container
  position: relative
  display inline-block
  clear both
  padding: 0 10px
  border-width: 1px
  border-style: solid
  border-color: #343131
  background: #f7f5ed
  background: -moz-linear-gradient(top,  #f7f5ed 0%, #ebe9e2 100%)
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7f5ed), color-stop(100%,#ebe9e2))
  background: -webkit-linear-gradient(top,  #f7f5ed 0%,#ebe9e2 100%)
  background: -o-linear-gradient(top,  #f7f5ed 0%,#ebe9e2 100%)
  background: -ms-linear-gradient(top,  #f7f5ed 0%,#ebe9e2 100%)
  border-radius(5px)
  box-shadow(0 0 3px rgba(0,0,0,.05))
  zoom: 1

._btn_gr.mara ._btn
  position: relative
  display: inline-block
  padding: 18px 22px
  text-align: center
  color: #36332e
  font-weight: 600
  font-size: 14px
  font-family: 'Open Sans', Arial
  text-transform: uppercase
  &:hover
    color blue

.img_gr.mara
  text-align center

.img_gr.mara a
  width 180px
  height 180px
  overflow hidden
  background: #f7f5ed
  display inline-block

.img_gr.mara .img
  max-width 300px
  max-height 300px
  display block


.footer_div.mara
  position relative
  margin-top 50px
  padding 45px 0 15px
  border-bottom 1px solid #cccac2
  background-color rgba(222,219,207,.7)
  &:after
    position absolute
    top -9px
    left 0
    width 100%
    height 9px
    background-image url('http://localhost:8080/static/bg/footer-top-arrow.png')
    background-repeat repeat-x
    content ""


.footer_div.mara .doc_div .doc_dis
  position relative
  display block
  padding-left 23px
  padding-bottom 3px
  background-repeat no-repeat




.main_nav.muru
  margin-top 35px
  center(max_width = 1410px)
  overflow hidden
  background $brown
  margin-bottom 15px
  border-radius(5px)
  box-shadow(2px 2px 3px rgba(0,0,0,.3))

.nav_title.muru
  float left

.nav_title.muru .title_h1
  float left
  margin-left 25px
  margin-right 25px
  font-size 31px
  font-weight 200
  color $text-white
  display inline-block
  @media screen and (max-width: 600px)
    display block
    text-align center
    float center
.nav_btn_list.muru
  float left
  margin-left 50px
  text-align center
  color $text-white
  @media screen and (max-width: 600px)
    display none
    width 100%
    text-align center
    padding 15px

.nav_btn_list.muru ._btn
  font-size 20px
  background $red
  color $text-white
  text-decoration none
  padding 33px 10px
  display inline-block
  font-weight 200
  box-sizing()
  transition(background .3s)

.nav_btn_list.muru ._btn:hover
  background $brown
  color $text-white
