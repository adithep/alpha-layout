@font-face
  font-family 'liga'
  src url('http://localhost:8080/static/font/liga/websymbolsligaregular.eot')
    , url('http://localhost:8080/static/font/liga/websymbolsligaregular.woff') format('woff')
    , url('http://localhost:8080/static/font/liga/websymbolsligaregular.ttf') format('truetype')
    , url('http://localhost:8080/static/font/liga/websymbolsligaregular.svg#web_symbols_ligaregular') format('svg')

@font-face
  font-family 'FontAwesome'
  src url('http://localhost:8080/static/font/awesome/fontawesome-webfont.eot?v=4.1.0')
  src url('http://localhost:8080/static/font/awesome/fontawesome-webfont.eot?#iefix&v=4.1.0') format('embedded-opentype')
    , url('http://localhost:8080/static/font/awesome/fontawesome-webfont.woff?v=4.1.0') format('woff')
    , url('http://localhost:8080/static/font/awesome/fontawesome-webfont.ttf?v=4.1.0') format('truetype')
    , url('http://localhost:8080/static/font/awesome/fontawesome-webfont.svg?v=4.1.0#fontawesomeregular') format('svg')
  font-weight normal
  font-style normal



body, html
  margin 0
  padding 0
  -webkit-font-smoothing antialiased
  font-family "Helvetica", arial
  font-weight normal
  font-size 16px
  color #666
a
  text-decoration none

span._btn:hover
  cursor pointer
